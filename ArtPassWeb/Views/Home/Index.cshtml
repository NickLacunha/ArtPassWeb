@section scripts {
    @Scripts.Render("~/bundles/app")
}

<div class="page-header">
    <h1>ArtPassAdmin</h1>
</div>

<div class="row">

    <div class="col-md-4">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h2 class="panel-title">Registrants</h2>
            </div>
            <div class="panel-body">
                <ul class="list-unstyled" data-bind="foreach: registrants">
                    <li>
                        <strong><span data-bind="text: Name"></span></strong>: 
                        <small><a href="#" data-bind="click: $parent.getBookDetail">Details</a></small>
                    </li>
                </ul>
            </div>
        </div>
        <div class="alert alert-danger" data-bind="visible: error"><p data-bind="text: error"></p></div>
    </div>

    @RenderPage("RegistrantDetails.cshtml")

    @RenderPage("AddRegistrant.cshtml")

</div>